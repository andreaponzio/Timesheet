{{#if data.error}}
   <div class="alert alert-danger alert-dismissible fade show mt-2" role="alert">
      <div>
         <i class="bi bi-exclamation-octagon"></i>
         {{data.error}}
      </div>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
   </div>
{{/if}}