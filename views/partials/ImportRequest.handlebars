<div class="modal fade" id="ImportRequest" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
     aria-labelledby="staticImportRequestLabel" aria-hidden="true">
   <div class="modal-dialog">
      <div class="modal-content">
         <div class="modal-header">
            <h1 class="modal-title fs-5">Importa dati trasporto</h1>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
         </div>
         <div class="modal-body">
            <label for="systemid" class="form-label">System ID</label>
            <input type="text" class="form-control css-uppercase" id="systemid" name="systemid" maxlength="3" required>
         </div>
         <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">
               <i class="bi bi-x-square"></i>
            </button>
            <button type="button" class="btn btn-success" onclick="modalRapidSubmit(2);">
               <i class="bi bi-box-arrow-in-down"></i>
            </button>
         </div>
      </div>
   </div>
</div>